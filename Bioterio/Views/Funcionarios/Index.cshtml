@model IEnumerable<Bioterio.Models.UserViewModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeCompleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Permissions)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomeCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(model => item.Permissions)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFuncionario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdFuncionario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFuncionario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
