@model Bioterio.Models.RegAlimentar
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-12">
    <h2>Detalhes do Registo Alimentar</h2>
</div>
<div class="col-md-4">
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Data)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.data)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Peso)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Peso)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Sobras)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Sobras)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.PlanoAlimentarIdPlanAlimNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PlanoAlimentarIdPlanAlimNavigation.Nome)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.TanqueIdTanqueNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TanqueIdTanqueNavigation.codidenttanque)
        </dd>
    </dl>
    <div class="form-group">
        <a type="button" class="btn btn-success" asp-action="Edit" asp-route-id="@Model.IdRegAlim">Editar</a>
        <a type="button" class="btn btn-danger pull-right" asp-action="Delete" asp-route-id="@Model.IdRegAlim">Eliminar</a>
        <a type="button" class="btn btn-info" asp-action="Index">Cancelarar</a>
    </div>
</div>
